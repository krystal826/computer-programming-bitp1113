// 3.2.3 Exp3 pg39.cpp : This program demostrates the relational and logical operators
//Author:Yoong Cheng Nee

#include<iostream>
using namespace std;

int main()
{
	int num1, num2;

	cout << "Enter two integers to know "
		<< "the relationship that they satisfy" << endl;
	cin >> num1 >> num2;

	if (num1 == num2)
		cout << num1 << " is equal to " << num2;
	else if ((num1 != num2) && (num1 < num2))
		cout << num1 << " is less than " << num2;
	else if ((num1 != num2) && (num1>num2))
		cout << num1 << " is greater than " << num2;
}

